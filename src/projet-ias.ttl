#################################################################
#    General axioms
#################################################################

[ rdf:type owl:AllDisjointClasses ;
  owl:members ( :Brand
                :Camera
                :Lens
                :Mount
                :Photography_Type
              )
] .


[ rdf:type owl:AllDifferent ;
  owl:distinctMembers ( :Event_photography
                        :Landscape_photography
                        :Night_photography
                        :Portrait_photography
                        :Sport_photography
                        :Street_photography
                        :Wildlife_photography
                      )
] .


#################################################################
#    Rules
#################################################################

:cam rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

:lens1 rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

:lens2 rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

:lens rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

:cam1 rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

:mnt rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

:cam2 rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

[ <http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> "true"^^xsd:boolean ;
   rdfs:comment "Define the mount of a camera by having a lens is compatible to an another camera" ;
   rdfs:label "r_mount_by_lens_compability" ;
   rdf:type <http://www.w3.org/2003/11/swrl#Imp> ;
   <http://www.w3.org/2003/11/swrl#body> [ rdf:type <http://www.w3.org/2003/11/swrl#AtomList> ;
                                           rdf:first [ rdf:type <http://www.w3.org/2003/11/swrl#IndividualPropertyAtom> ;
                                                       <http://www.w3.org/2003/11/swrl#propertyPredicate> :hasMount ;
                                                       <http://www.w3.org/2003/11/swrl#argument1> :cam1 ;
                                                       <http://www.w3.org/2003/11/swrl#argument2> :mnt
                                                     ] ;
                                           rdf:rest [ rdf:type <http://www.w3.org/2003/11/swrl#AtomList> ;
                                                      rdf:first [ rdf:type <http://www.w3.org/2003/11/swrl#IndividualPropertyAtom> ;
                                                                  <http://www.w3.org/2003/11/swrl#propertyPredicate> :isCompatibleWith ;
                                                                  <http://www.w3.org/2003/11/swrl#argument1> :lens ;
                                                                  <http://www.w3.org/2003/11/swrl#argument2> :cam1
                                                                ] ;
                                                      rdf:rest [ rdf:type <http://www.w3.org/2003/11/swrl#AtomList> ;
                                                                 rdf:first [ rdf:type <http://www.w3.org/2003/11/swrl#IndividualPropertyAtom> ;
                                                                             <http://www.w3.org/2003/11/swrl#propertyPredicate> :isCompatibleWith ;
                                                                             <http://www.w3.org/2003/11/swrl#argument1> :lens ;
                                                                             <http://www.w3.org/2003/11/swrl#argument2> :cam2
                                                                           ] ;
                                                                 rdf:rest rdf:nil
                                                               ]
                                                    ]
                                         ] ;
   <http://www.w3.org/2003/11/swrl#head> [ rdf:type <http://www.w3.org/2003/11/swrl#AtomList> ;
                                           rdf:first [ rdf:type <http://www.w3.org/2003/11/swrl#IndividualPropertyAtom> ;
                                                       <http://www.w3.org/2003/11/swrl#propertyPredicate> :hasMount ;
                                                       <http://www.w3.org/2003/11/swrl#argument1> :cam2 ;
                                                       <http://www.w3.org/2003/11/swrl#argument2> :mnt
                                                     ] ;
                                           rdf:rest rdf:nil
                                         ]
 ] .

[ <http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> "true"^^xsd:boolean ;
   rdfs:comment "Define if a Lens is for DSLR_Camera by checking others Lens that are compatible with the camera are for DSLR" ;
   rdfs:label "r_lens_for_dslr_by_similarity" ;
   rdf:type <http://www.w3.org/2003/11/swrl#Imp> ;
   <http://www.w3.org/2003/11/swrl#body> [ rdf:type <http://www.w3.org/2003/11/swrl#AtomList> ;
                                           rdf:first [ rdf:type <http://www.w3.org/2003/11/swrl#ClassAtom> ;
                                                       <http://www.w3.org/2003/11/swrl#classPredicate> :DSLR_Camera ;
                                                       <http://www.w3.org/2003/11/swrl#argument1> :cam
                                                     ] ;
                                           rdf:rest [ rdf:type <http://www.w3.org/2003/11/swrl#AtomList> ;
                                                      rdf:first [ rdf:type <http://www.w3.org/2003/11/swrl#IndividualPropertyAtom> ;
                                                                  <http://www.w3.org/2003/11/swrl#propertyPredicate> :isCompatibleWith ;
                                                                  <http://www.w3.org/2003/11/swrl#argument1> :cam ;
                                                                  <http://www.w3.org/2003/11/swrl#argument2> :lens1
                                                                ] ;
                                                      rdf:rest [ rdf:type <http://www.w3.org/2003/11/swrl#AtomList> ;
                                                                 rdf:first [ rdf:type <http://www.w3.org/2003/11/swrl#IndividualPropertyAtom> ;
                                                                             <http://www.w3.org/2003/11/swrl#propertyPredicate> :isCompatibleWith ;
                                                                             <http://www.w3.org/2003/11/swrl#argument1> :cam ;
                                                                             <http://www.w3.org/2003/11/swrl#argument2> :lens2
                                                                           ] ;
                                                                 rdf:rest [ rdf:type <http://www.w3.org/2003/11/swrl#AtomList> ;
                                                                            rdf:first [ rdf:type <http://www.w3.org/2003/11/swrl#DifferentIndividualsAtom> ;
                                                                                        <http://www.w3.org/2003/11/swrl#argument1> :lens1 ;
                                                                                        <http://www.w3.org/2003/11/swrl#argument2> :lens2
                                                                                      ] ;
                                                                            rdf:rest [ rdf:type <http://www.w3.org/2003/11/swrl#AtomList> ;
                                                                                       rdf:first [ rdf:type <http://www.w3.org/2003/11/swrl#ClassAtom> ;
                                                                                                   <http://www.w3.org/2003/11/swrl#classPredicate> :Lens_For_DSLR ;
                                                                                                   <http://www.w3.org/2003/11/swrl#argument1> :lens1
                                                                                                 ] ;
                                                                                       rdf:rest rdf:nil
                                                                                     ]
                                                                          ]
                                                               ]
                                                    ]
                                         ] ;
   <http://www.w3.org/2003/11/swrl#head> [ rdf:type <http://www.w3.org/2003/11/swrl#AtomList> ;
                                           rdf:first [ rdf:type <http://www.w3.org/2003/11/swrl#ClassAtom> ;
                                                       <http://www.w3.org/2003/11/swrl#classPredicate> :Lens_For_DSLR ;
                                                       <http://www.w3.org/2003/11/swrl#argument1> :lens2
                                                     ] ;
                                           rdf:rest rdf:nil
                                         ]
 ] .

[ <http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> "true"^^xsd:boolean ;
   rdfs:comment "Define if a Lens is for DSLR_Camera" ;
   rdfs:label "r_lens_for_dslr" ;
   rdf:type <http://www.w3.org/2003/11/swrl#Imp> ;
   <http://www.w3.org/2003/11/swrl#body> [ rdf:type <http://www.w3.org/2003/11/swrl#AtomList> ;
                                           rdf:first [ rdf:type <http://www.w3.org/2003/11/swrl#IndividualPropertyAtom> ;
                                                       <http://www.w3.org/2003/11/swrl#propertyPredicate> :isCompatibleWith ;
                                                       <http://www.w3.org/2003/11/swrl#argument1> :lens ;
                                                       <http://www.w3.org/2003/11/swrl#argument2> :cam
                                                     ] ;
                                           rdf:rest [ rdf:type <http://www.w3.org/2003/11/swrl#AtomList> ;
